{
  "ConnectionStrings": {
    "GymDbContext": "Host=localhost;Database=YOUR_DATABASE;Username=YOUR_USERNAME;Password=YOUR_PASSWORD"
  },
  "Data": {
    "Gym": {
      "MigrationsAssembly": "GymSystem.Persistance"
    }
  },
  "AI": {
    "Gemini": {
      "ApiKey": "YOUR_GEMINI_API_KEY"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5001"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
